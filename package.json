  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xan105/node-cgo-regodit"
  },
  "bugs": {
    "url": "https://github.com/xan105/node-cgo-regodit/issues"
  },
  "homepage": "https://github.com/xan105/node-cgo-regodit#readme",
  "dependencies": {
    "ffi-napi": "^4.0.3"
  },
  "devDependencies": {
    "eslint": "^7.32.0"
  }
}
