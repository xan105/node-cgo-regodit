{
  "name": "regodit",
  "version": "2.0.1",
  "description": "Access Windows registry via a cgo dll (ffi-napi)",
  "type": "module",
  "exports": {  
    ".": "./lib/esm.js",
    "./promises": "./lib/promises.js"
  },
  "types": "./types/esm.d.ts",
  "os": [
    "win32"
  ],
  "cpu": [
    "x64"
  ],
  "engines": {
    "node": ">=0.14"
  },
  "scripts": {
    "lint": "npx eslint --ignore-path .gitignore ./**/*.{js,mjs}",
    "build": "call lib\\go\\build.cmd"
  },
  "keywords": [
    "windows",
    "registry",
    "ffi",
    "golang",
    "cgo"
  ],
  "author": {
    "name": "Anthony Beaumont",
